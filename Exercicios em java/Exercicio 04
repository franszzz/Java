// Questão 04: Fazer um programa para ler quatro valores inteiros A, B, C e D. A seguir, calcule e mostre a diferença do produto de A e B pelo produto de C e D segundo a fórmula: DIFERENCA = (A * B - C * D).
package javiando;
import java.util.Scanner;

public class javajan {

	public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
		System.out.println("Digite o primeiro valor");
		 int a = input.nextInt();
		System.out.println("Digite o segundo valor");
		 int b = input.nextInt();
		System.out.println("Digite o terceiro valor");
		 int c = input.nextInt();
		System.out.println("Digite o quarto valor");
		 int d = input.nextInt();
		 
		 int dif = a*b-c*d;
		
		
	System.out.println("A diferença é " + dif);
	
	input.close();

	}
}
